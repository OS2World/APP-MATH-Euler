MENU ID_Menu 
BEGIN
	SUBMENU "~File", 100
	BEGIN
		MENUITEM "~Load ...\tF10", IDM_Load
		MENUITEM "Load ~Demo", IDM_LoadDemo
		MENUITEM SEPARATOR
		MENUITEM "Ne~w Notebook", IDM_NewNotebook
		MENUITEM "Load ~Notebook ...\tAlt-F9", IDM_LoadNotebook
		MENUITEM "~Save Notebook\tAlt-F10", IDM_SaveNotebook
		MENUITEM "Save Notebook ~as ...", IDM_SaveNotebookAs
		MENUITEM SEPARATOR
		MENUITEM "File=...", IDM_SetFile
		MENUITEM "Load ~File\tF4", IDM_LoadFile
		MENUITEM SEPARATOR
		MENUITEM "St~ore Session ...", IDM_Store
		MENUITEM "~Restore Session ...", IDM_Restore
