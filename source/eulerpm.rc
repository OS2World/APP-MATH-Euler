		BEGIN
			MENUITEM "As ~Bitmap ...", IDM_SaveGraphicsBitmap
			MENUITEM "As ~Metafile ...", IDM_SaveGraphicsMeta
		END

		MENUITEM SEPARATOR
		MENUITEM "~Print Graphics", IDM_PrintGraphics
		MENUITEM SEPARATOR
		MENUITEM "E~xit", IDM_Exit
	END

	SUBMENU "~Edit", 200
	BEGIN
		MENUITEM "~Copy All\tCtrl-Ins", IDM_CopyAll
		MENUITEM "C~opy Commands\tShift-F6", IDM_CopyCommands
		MENUITEM "~Paste All\tShift-Ins", IDM_PasteAll
		MENUITEM "P~aste Commands\tShift-F7", IDM_PasteCommands
		MENUITEM SEPARATOR
		MENUITEM "~Delete Output\tShift-Del", IDM_Cut
		MENUITEM SEPARATOR
		SUBMENU "Copy ~Graphics"
		BEGIN
			MENUITEM "As ~Bitmap", IDM_CopyGraphicsB
			MENUITEM "As  ~Metafile", IDM_CopyGraphicsM
		END

	END

	SUBMENU "~Notebook", 500
	BEGIN
		MENUITEM "~Previous Command\tUp", IDM_PreviousCommand
		MENUITEM "~Next Command\tDown", IDM_NextCommand
		MENUITEM SEPARATOR
		MENUITEM "~Edit Command Text\tF5", IDM_EditText
		MENUITEM "~Delete Command Text\tShift-F5", IDM_DeleteText
		MENUITEM SEPARATOR
		MENUITEM "De~lete Command\tAlt-Delete", IDM_DeleteCommand
		MENUITEM "In~sert Command\tAlt-Ins", IDM_InsertCommand
		MENUITEM SEPARATOR
		MENUITEM "~Remove all Output", IDM_RemoveOutput
		MENUITEM SEPARATOR
		MENUITEM "Insert ~Commands", IDM_InsertCommands
	END

	SUBMENU "~Options", 300
	BEGIN
		MENUITEM "~Edit Text\tF9", IDM_Function
		MENUITEM "Call ~Reset", IDM_Reset
		MENUITEM SEPARATOR
		MENUITEM "~B/W graphics only", IDM_BW
		MENUITEM "~Math errors", IDM_MathErrors
		MENUITEM "~Notebook Colors", IDM_NotebookColors
		MENUITEM "Compute on ~Idle Time", IDM_Idletime
		SUBMENU "~Set"
		BEGIN
			MENUITEM "~Fonts", IDM_SetFonts
			MENUITEM "~Sizes", IDM_SetSizes
			MENUITEM "~Graphics", IDM_SetGraphics
			MENUITEM "~Print", IDM_SetPrint
			MENUITEM "P~ath", IDM_SetPaths
		END

		MENUITEM "Save ~Windows", IDM_SaveWindows
		MENUITEM "S~ave Settings", IDM_SaveSettings
	END

	SUBMENU "~Help", 400
	BEGIN
		MENUITEM "~General Help\tF1", IDM_General
		MENUITEM "Help ~Index\tF2", IDM_Index
		MENUITEM "Help ~Contents\tF3", IDM_Content
